{
    "env": {
        "es6": true,
        "commonjs": true
    },
    "extends": ["eslint:recommended"],
    "rules": {
        "no-console": 0,
        "quotes": ["error", "single"],
        "indent": "error"        
    },
    "globals": {
        "window": false,
        "console": false,
        "document": false,
        "XPathResult": false,
        "process": false,
        "setTimeout": false
    }
}